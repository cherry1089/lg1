<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="16dp"
    android:transitionName="expandDetails"
    app:cardCornerRadius="8dp"
    app:cardElevation="10dp"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#B9BAB9B9"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/vehicleType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/amaranth"
                    android:paddingBottom="5dp"
                    android:text="Car"
                    android:textSize="30sp" />
             <TextView
                    android:id="@+id/BrandModel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:fontFamily="@font/amaranth"
                    android:paddingBottom="5dp"
                    android:text="Honda,grandi10"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/locationOfVehicle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:fontFamily="@font/amaranth"
                    android:paddingBottom="5dp"
                    android:text="Vijayawada"
                    android:textSize="20sp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="Fpd:"
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/Cost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="5000"
                        android:textSize="20sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="Seater"
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/seaterNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="4"
                        android:textSize="20sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="@string/owner"
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/owner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="@string/maple"
                        android:textSize="20sp" />
                </LinearLayout>
                <TextView
                    android:id="@+id/mail1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:fontFamily="@font/amaranth"
                    android:paddingBottom="5dp"
                    android:text="mail.mail@gmail.com"
                    android:textSize="15sp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="@string/phone_no"
                        android:textSize="15sp" />
                    <TextView
                        android:id="@+id/pno1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="@string/_59532535"
                        android:textSize="15sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/sdate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/amaranth"
                        android:text="1/1/1111"
                        android:textSize="20sp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/amaranth"
                        android:text=" - "
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/edate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/amaranth"
                        android:text="2/2/2222"
                        android:textSize="20sp" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/hostedImage"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_gravity="start"
                    android:src="@drawable/car" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="Rnumber"
                        android:textSize="15sp" />
                    <TextView
                        android:id="@+id/rno1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:fontFamily="@font/amaranth"
                        android:paddingBottom="5dp"
                        android:text="1234"
                        android:textSize="15sp" />
                    <TextView
                        android:id="@+id/renterno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" "
                        android:textSize="1sp" />
                </LinearLayout>
                <Button
                    android:id="@+id/biq"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#A62C2929"
                    android:fontFamily="@font/bungee"
                    android:gravity="center"
                    android:text="@string/rent"
                    android:textColor="#FFFFFF"
                    android:textSize="10dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</com.google.android.material.card.MaterialCardView>